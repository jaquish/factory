# Comments start with #
# [] means a comma-delimited list, no spaces

@Metadata
Format-Version:<string>
Name:<string>

@Widges
[default]
<widge id> <sprite name>

@Actions
<id> <[input widgeTypeIDs]> <[success widgeTypeIDs]> <[failure widgeTypeIDs]>

@Machines
<class name> ...initializer parameters

@Context
inputs          <[widgeTypeIDs]>
winning-outputs <[widgeTypeIDs]>
endgame-output-count <#>